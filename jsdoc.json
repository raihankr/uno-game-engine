{
    "plugins": [
        "plugins/markdown",
        "./node_modules/jsdoc-export-default-interop/dist/index"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "index.js",
            "src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)[_\\.]?(.+\\.test.js(doc|x)?)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "name": "Uno Game Engine",
        "footerText": "Â©2024 Raihan Khairul Rochman",
        "useCollapsibles": true,
        "logo": {
            "url": "img/github-mark-white.png",
            "width": "13px",
            "height": "13px",
            "link": "https://github.com/raihankr/uno-game-engine"
        }
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "readme": "README.md",
        "package": "package.json",
        "template": "node_modules/tui-jsdoc-template",
        "destination": "../Uno_Game_Engine_Docs"
    }
}